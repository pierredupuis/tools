alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gck='git checkout'
alias gckb='git checkout -b'
alias gm='git merge --no-ff'
alias gl='git log'
alias glg='git log --oneline --graph'
alias gr='git reflog'
alias gst='git stash'
alias gp='git push'
alias gd='git diff'
alias gdh='git diff HEAD'
